{
  "name": "bfx-api-node-plugin-managed-ob",
  "version": "1.0.2",
  "description": "Maintains complete local order books for the Bitfinex Node API",
  "engines": {
    "node": ">=7"
  },
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test nyc --check-coverage --lines 85 --branches 70 --functions 80 --statements 80 --reporter=lcov --reporter=html mocha -b --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-api-node-plugin-managed-ob.git"
  },
  "keywords": [
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-api-node-plugin-managed-ob/issues"
  },
  "dependencies": {
    "bfx-api-node-core": "^1.1.0",
    "bfx-api-node-models": "^1.0.12",
    "bluebird": "^3.5.5",
    "debug": "^4.1.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.1.0"
  }
}
